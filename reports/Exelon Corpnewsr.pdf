<!-- SEARCH AUTOCOMPLETE-->
<script id="search-autocomplete" type="text/html">
    <div class="search-autocomplete" style="display: inline;" data-bind="expandEditor: showFlyout">
        <input id="searchBox" type="text" placeholder="Search..." autocomplete="off" data-bind="value: searchValue, valueUpdate: 'afterkeydown', event: { keyup: attemptSearch }, hasFocus: function (data) { readyForFlyout(true); }" />
        <div class="selected-value" data-bind="text: selectedValue"></div>
        <div class="search-result-dropdown" data-bind="visible: showFlyout" style="display: none;">
            <a href="#" class="btn-close" title="Close" data-bind="click: function (data) { closeFlyout(); }"></a>
            <!-- ko if: showSpinner -->
            <div data-bind="spinner: showSpinner"></div>
            <!-- /ko -->
            <div class="search-result-category" data-bind="visible: !showSpinner()">
                <p class="no-results" data-bind="visible: searchResults().length == 0">No Results Found</p>
                <ul data-bind="foreach: searchResults">
                    <li>
                        <a href="#" data-bind="attr: { title: $data.title }, text: $data.title, click: function (data) { $root.changeDataSource(data, $root, $parent); $parent.searchResultClicked(data); }"></a>
                    </li>
                </ul>
            </div>
        </div>
    </div>
</script>
<!-- /SEARCH AUTOCOMPLETE-->

<!-- MEGANAV SEARCH AUTOCOMPLETE-->
<script id="meganav-search-autocomplete" type="text/html">
    <li class="dropdown nav-search">
        <a href="#" title="Open Menu" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-expanded="false" data-bind="click: function() { setFocusWhenVisible($('.header-search'))}"></a>
        <div class="dropdown-menu">
            <div class="input-container">
                <input type="text" class="header-search" placeholder="Search Exeloncorp.com" data-bind="textInput: searchValue, valueUpdate: 'afterkeydown', event: { keyup: attemptSearch }" />
                <button class="header-search-submit" disabled="disabled" data-bind="click: function () { redirectToSearchPage(); }"></button>
            </div>
                            
            <div class="clearfix"></div>
            
            <!-- ko if: showFlyout() && suggestedSearch().length > 0 -->
            <div class="type-ahead">
                <a href="#" class="btn-close" title="Close" data-bind="click: function (data, event) { event.preventDefault(); event.stopPropagation(); closeFlyout(); }"></a>
                <div data-bind="spinner: showSpinner"></div>
                <!-- ko ifnot: showSpinner -->
                <ul data-bind="foreach: suggestedSearch">
                    <li data-bind="text: $data, click: function (data) { $root.changeDataSource(data, $root, $parent); $parent.searchResultClicked(data); }"></li>
                </ul>
                
                <!-- /ko -->
            </div>
            <!-- /ko -->
            
        </div>
        
    </li>
</script>
<!-- /MEGANAV SEARCH AUTOCOMPLETE-->

<script id="taxonomy-filter" type="text/html">
    <!-- desktop/tablet -->
    <div class="taxonomy-filters">
        <div class="filter-headers" data-bind="foreach: taxonomies">
            <!-- ko if: children().length > 0 -->
            <span class="taxonomy-filter-label" data-bind="text: name"></span>
            <!-- /ko -->
        </div>
        <div class="taxonomy-options" data-bind="foreach: taxonomies">
            <div data-bind="foreach: children">
                <div>
                    <label class="checkbox">
                        <input type="checkbox" data-bind="value: $data, checked: $parent.selectedChildren" />
                        <span class="checkbox"></span><span class="taxonomy-option-label" data-bind="text: name"></span>
                    </label>
                </div>
            </div>
        </div>
        <div class="filter-btn-group">
            <a href="javascript:void(0);" class="btn btn-apply-filters" title="Apply Filters" data-bind="click: setSelectedOptions">Apply Filters</a>
            <a href="javascript:void(0);" class="btn btn-cancel-filters" title="Cancel" data-bind="click: resetSelectedOptions">Cancel</a>
        </div>
    </div>

    <!-- mobile -->
    <div class="taxonomy-filters-mobile" data-bind="collapseOnClickOutside: 'filters'">
        <div class="filter-header">
            <h3>Filter Results</h3>
        </div>
        <div class="flex-box">
            <div id="filter-accordion" class="taxonomy-list-mobile" data-bind="foreach: taxonomies, accordion: { heightStyle: 'content', collapsible: true }">
                <h3 class="taxonomy-filter-label" data-bind="text: name, click: $parent.setActiveList.bind($parent), css: { 'active': $parent.activeTaxonomy() === $data }"></h3>
                <!-- accordion content -->
                <div>
                    <!-- ko foreach: children -->
                    <div class="filters">
                        <label class="checkbox">
                            <input type="checkbox" data-bind="value: $data, checked: $parent.selectedChildren" />
                            <span class="checkbox"></span><span class="taxonomy-option-label" data-bind="text: name"></span>
                        </label>
                    </div>
                    <!-- /ko -->
                </div>
            </div>
            <!-- /.taxonomy-list-mobile -->
        </div>
        <!-- /.flex-box -->
        <div class="filter-btn-group">
            <a href="javascript:void(0);" class="btn btn-apply-filters" title="Apply Filters and Close" data-bind="click: setSelectedOptions">Apply Filters & Close</a>
        </div>
    </div>
</script>

<!-- JOBLISTING EDITOR -->
<script id="joblisting-editor" type="text/html">
    <div class="editor row">
        <div class="col-md-2">
            Title<br />
            <input type="text" data-bind="value: title" />
        </div>
        <div class="col-md-2">
            Items per Page<br />
            <input type="text" data-bind="value: itemsPerPage" /><br />
        </div>
        <div class="col-md-2">
            Show recent jobs if query returns no results<br />
            <input type="checkbox" data-bind="checked: enableQueryFallback" />
        </div>
        <div class="col-md-2">
            Select a Query:<br />
            <select data-bind="options: queryOptions, optionsText: 'text', value: selectedQuery"></select>
            <!-- ko if: selectedQuery() && selectedQuery().value === Exelon.JobListingQueryOption.ByEnergyType -->
            <br />
            <br />
            <input type="text" data-bind="value: energyType" placeholder="Energy Type"/>
            <!-- /ko -->
            <!-- ko if: selectedQuery() && selectedQuery().value === Exelon.JobListingQueryOption.Custom -->
            <br />
            <br />
            <input type="button" data-bind="click: buildCustomQuery.bind($data)" value="Set Query"/>
            <!-- /ko -->
        </div>
    </div>
</script>
<!-- /JOBLISTING EDITOR -->

<!-- GROUP TERM HOVER -->
<script type="text/html" id="group-term-hover-template">
    <!-- ko if: termHovers() && termHovers().length -->
    <div class="mobile-buttons">
        <div class="exe-container controls">
            <div class="exelon-groupHover-controls" data-bind="foreach: termHovers">
                <a class="exelon-groupHover-button" data-bind="attr: { 'href': url, 'title': term }">
                    <span data-bind="text: term"></span>
                </a>
            </div>
        </div>
    </div>
    <!-- /.mobile-buttons -->

    <div class="desktop-tablet">
        <!-- controls -->
        <div class="exe-container controls">
            <div class="exelon-groupHover-controls" data-bind="foreach: termHovers">
                <button class="exelon-groupHover-button" data-bind="click: $parent.toggleTermHover, clickBubble: false, css: { active: showHover() }">
                    <span class="bg"></span>
                    <span data-bind="text: term"></span>
                </button>
            </div>
        </div>
        <!-- /.exe-container -->

        <!-- content -->
        <div class="exelon-groupHover-page" data-bind="foreach: termHovers">
            <div class="exe-container" data-bind="attr: { id: term.replace(/ /g, '-') }">
                <div class="exelon-groupHover" data-bind="fadeVisible: showHover">
                    <div data-bind="spinner: showSpinner"></div>
                    <!-- ko if: !loadingProfiles() -->
                    <button class="nav-button prev" data-bind="click: prev, clickBubble: false, visible: currentPage() > 1"></button>
                    <button class="nav-button next" data-bind="click: next, clickBubble: false, visible: nextPageEnabled()"></button>
                    <!-- /ko -->
                    <ul data-bind="foreach: { data: displayedProfiles, afterRender: logLoad.bind($data) }, visible: !showSpinner()">
                        <!-- ko if: !$root.isCurrentProfile(profile().termId, profile().url) -->
                        <li>
                            <div class="exelon-groupHoverProfile" data-bind="css: { 'active-profile': $root.isCurrentProfile(profile().termId, profile().url) }">
                                <a data-bind="attr: { href: $root.isCurrentProfile(profile().termId, profile().url) ? 'javascript:void(0);' : profile().url, title: profile().personInfo.fullName }">
                                    <span class="image" data-bind="html: profile().image"></span>
                                    <div class="profile-content">
                                        <p class="name"><span data-bind="    text: profile().personInfo.fullName"></span></p>
                                        <div class="titleCompany" data-bind="foreach: titles">
                                            <p class="title" data-bind="text: $data"></p>
                                        </div>

                                    </div>
                                </a>
                                <!-- /.profile-content -->
                            </div>
                        </li>
                        <!-- /ko -->
                    </ul>
                </div>
                <!-- /.exelon-groupHover -->
            </div>
            <!-- /.exe-container -->
        </div>
        <!-- /.exelon-groupHover-page -->

    </div>
    <!-- /.desktop-tablet -->
    <!-- /ko -->
</script>
<!-- /GROUP TERM HOVER -->

